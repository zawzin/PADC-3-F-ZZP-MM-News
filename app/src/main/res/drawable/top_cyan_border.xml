<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="@color/cyan" />
        </shape>
    </item>
    <item
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="@color/white_full"/>
        </shape>
    </item>
    <item
        android:top="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#BDBDBD" />
        </shape>
    </item>
    <item
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="@color/white_full"/>
            <solid android:color="@color/white_full"/>
        </shape>
    </item>
</layer-list>